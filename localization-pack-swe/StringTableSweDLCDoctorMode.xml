            <GameDBLocalizedString>      <LocID>DLC_ACHIEVEMENT_CREATE_ONE_CHAR</LocID>                     <Text>I choose you...</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>      <LocID>DLC_ACHIEVEMENT_CREATE_ONE_CHAR_DESCRIPTION</LocID>         <Text>Skapa din egna karaktär.</Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>      <LocID>DLC_ACHIEVEMENT_CREATE_MORE_CHAR</LocID>                    <Text>...and you and you and you too.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>      <LocID>DLC_ACHIEVEMENT_CREATE_MORE_CHAR_DESCRIPTION</LocID>        <Text>Skapa 10 olika karaktärer.</Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>      <LocID>DLC_ACHIEVEMENT_SPECIALIST_MODE</LocID>                     <Text>Let's put the skills to the test</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>      <LocID>DLC_ACHIEVEMENT_SPECIALIST_MODE_DESCRIPTION</LocID>         <Text>Aktivera svårighetsgraden Specialist</Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>      <LocID>DLC_ACHIEVEMENT_SPECIALIST_MODE_PATIENT</LocID>             <Text>You didn't see anything</Text>  </GameDBLocalizedString>
